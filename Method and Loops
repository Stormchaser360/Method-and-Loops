    <h2>Numbers</h2>
    <p id="demo"></p>
    <script>
        var fruits = ["10", "21", "13", "43", "15", "65"];
        var text = "";
        var i;
        for (i = 0; i < fruits.length; i++) {
            text += fruits[i] + "<br>";
        }
        document.getElementById("demo").innerHTML = text;
    </script>
